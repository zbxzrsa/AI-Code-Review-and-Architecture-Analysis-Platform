# Database
DATABASE_URL=postgresql+asyncpg://user:password@postgres:5432/analysis_db

# Redis (for cache and Celery result backend)
REDIS_URL=redis://redis:6379/0
REDIS_CACHE_URL=redis://redis:6379/1

# Celery / RabbitMQ
CELERY_BROKER_URL=amqp://guest:guest@rabbitmq:5672//
CELERY_RESULT_BACKEND=redis://redis:6379/1
CELERY_TASK_ACKS_LATE=true
CELERY_TASK_REJECT_ON_WORKER_LOST=true
CELERY_WORKER_CONCURRENCY=4
CELERYD_PREFETCH_MULTIPLIER=1
TASK_TIME_LIMIT=900
TASK_SOFT_TIME_LIMIT=840

# S3 / MinIO
S3_ENDPOINT_URL=http://minio:9000
S3_ACCESS_KEY_ID=MINIOACCESS
S3_SECRET_ACCESS_KEY=MINIOSECRET
S3_REGION=us-east-1
S3_FORCE_PATH_STYLE=true
S3_SECURE=false
ARTIFACTS_BUCKET=artifacts

# Presigned URLs
ARTIFACTS_PRESIGN_DOWNLOAD_ENABLED=true
ARTIFACTS_PRESIGN_DOWNLOAD_TTL=86400
ARTIFACTS_PRESIGN_UPLOAD_ENABLED=false
ARTIFACTS_PRESIGN_UPLOAD_TTL=900

# Cache
CACHE_TTL_DAYS=7
WORKER_DEP_CLOSURE_LIMIT=500

# Sandbox / Execution
SANDBOX_CPU_LIMIT=2
SANDBOX_MEM_LIMIT_MB=2048

# Monitoring
PROMETHEUS_PORT=9464
LOG_LEVEL=INFO
